                "script": "#!/bin/zsh\n\nif [ -z \"$APPEND_COLUMN\" ]; then\ncat << EOB\n{\"items\": [\n\n\t{\n\t\t\"title\": \"\ud83d\udea8 Error: Append column not set\",\n\t\t\"subtitle\": \"Check Workflow configuration\"\n\t\t}\n\t\n\n\t\n\n]}\nEOB\n\n\nelse\nmyText=$1\nmyTextLength=${#myText}\n\n\n\ncat << EOB\n{\"items\": [\n\n\t{\n\t\t\"title\": \"Appending to column $APPEND_COLUMN: $1\",\n\t\t\"subtitle\": \"length: $myTextLength\",\n\t\t\"arg\": \"$1\"\n\t\t}\n\t\n\n\t\n\n]}\nEOB\nfi",
